<Settings>
  <pollingRate>-1</pollingRate>
  <folderLocation>extracted\\</folderLocation>
  <BeanList>
    <Bean>
      <name>org.jmxdatamart:name=CarBean</name>
      <alias>Applesauce</alias>
      <AttributeList>
        <Attribute>
          <name>*</name>
        </Attribute>
        <Attribute>
          <name>*.*</name>
        </Attribute>
        <Attribute>
          <name>*.*.*</name>
        </Attribute>
      </AttributeList>
      <enable>true</enable>
    </Bean>
  </BeanList>
</Settings>