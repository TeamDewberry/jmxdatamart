<Settings>
  <pollingRate>1000</pollingRate>
  <folderLocation>${fitnesse.working}/jmx-statistics</folderLocation>
  <BeanList class="singleton-list">
    <Bean>
      <name>org.jmxdatamart:Type=TestWebAppMBean</name>
      <alias>TestWebAppMBean</alias>
      <AttributeList class="singleton-list">
        <Attribute>
          <dataType>INT</dataType>
          <name>Age</name>
          <alias>age</alias>
        </Attribute>
      </AttributeList>
      <enable>true</enable>
    </Bean>
  </BeanList>
</Settings>
