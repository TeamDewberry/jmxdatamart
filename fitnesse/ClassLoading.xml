<!--
ClassLoading Example
based on David's example
-->
<Settings>
  <pollingRate>-1</pollingRate><!-- for fitness testing -->
  <folderLocation>extracted\\</folderLocation>
  <url></url>
  <BeanList>
    <Bean>
      <name>java.lang:type=ClassLoading</name>
      <alias>ClassLoading</alias>
      <AttributeList>
        <Attribute>
          <dataType>BOOLEAN</dataType>
          <name>Verbose</name>
        </Attribute>
        <Attribute>
          <dataType>INT</dataType>
          <name>LoadedClassCount</name>
        </Attribute>
        <Attribute>
          <dataType>LONG</dataType>
          <name>UnloadedClassCount</name>
        </Attribute>
        <Attribute>
          <dataType>LONG</dataType>
          <name>TotalLoadedClassCount</name>
        </Attribute>
      </AttributeList>
      <enable>true</enable>
    </Bean>
  </BeanList>
</Settings>